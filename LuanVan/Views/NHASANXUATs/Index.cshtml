@model IEnumerable<LuanVan.Models.NHASANXUAT>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Danh sách</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NSX_TEN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NSX_SDT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NSX_QUOCGIA)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NSX_GHICHU)
        </th>
        <th>Tùy chọn</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NSX_TEN)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NSX_SDT)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NSX_QUOCGIA)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NSX_GHICHU)
        </td>
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id=item.NSX_ID }) |
            @Html.ActionLink("Chi tiết", "Details", new { id=item.NSX_ID }) |
            @Html.ActionLink("Xóa", "Delete", new { id=item.NSX_ID })
        </td>
    </tr>
}

</table>
